<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/black">

    <TextView
        android:id="@+id/textView90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sf_pro_text_bold"
        android:text="@string/payment_details"
        android:textColor="@color/white"
        android:textSize="@dimen/size15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view30_locations"
        android:layout_width="0dp"
        android:layout_height="57dp"
        app:layout_constraintEnd_toStartOf="@+id/view32_screens"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView90" />

    <View
        android:id="@+id/view32_screens"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/view30_locations"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toEndOf="@+id/view30_locations"
        app:layout_constraintTop_toTopOf="@+id/view30_locations" />

    <TextView
        android:id="@+id/tvBookingId"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:includeFontPadding="false"
        android:text="@string/booking_id"
        android:textColor="@color/hint_color"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/view30_locations"
        app:layout_constraintStart_toStartOf="@+id/view30_locations"
        app:layout_constraintTop_toTopOf="@+id/view30_locations" />

    <TextView
        android:id="@+id/tvBookingIdNumber"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sf_pro_text_regular"
        android:layout_marginTop="@dimen/margin4"
        android:lineSpacingExtra="9sp"
        android:text="WES096T"
        android:textColor="@color/white"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/tvBookingId"
        app:layout_constraintStart_toStartOf="@+id/tvBookingId"
        app:layout_constraintTop_toBottomOf="@+id/tvBookingId"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/tvTrackId"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:includeFontPadding="false"
        android:text="@string/track_id"
        android:textColor="@color/hint_color"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/view32_screens"
        app:layout_constraintStart_toStartOf="@+id/view32_screens"
        app:layout_constraintTop_toTopOf="@+id/view32_screens" />

    <TextView
        android:id="@+id/tvTrackIdNumber"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sf_pro_text_regular"
        android:layout_marginTop="@dimen/margin4"
        android:lineSpacingExtra="9sp"
        tools:text="WsRT87T"
        android:textColor="@color/white"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/tvTrackId"
        app:layout_constraintStart_toStartOf="@+id/tvTrackId"
        app:layout_constraintTop_toBottomOf="@+id/tvTrackId" />

    <View
        android:id="@+id/view33_date"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toStartOf="@+id/view34_time"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toStartOf="@+id/view30_locations"
        app:layout_constraintTop_toBottomOf="@+id/view30_locations" />

    <View
        android:id="@+id/view34_time"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/view33_date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toEndOf="@+id/view33_date"
        app:layout_constraintTop_toTopOf="@+id/view33_date" />

    <TextView
        android:id="@+id/tvReferenceId"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sf_pro_text_regular"
        android:includeFontPadding="false"
        android:text="@string/reference_id"
        android:textColor="@color/hint_color"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/view33_date"
        app:layout_constraintStart_toStartOf="@+id/view33_date"
        app:layout_constraintTop_toTopOf="@+id/view33_date" />

    <TextView
        android:id="@+id/tvReferenceIdInfo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:lineSpacingExtra="9sp"
        android:text="WECTFG34"
        android:textColor="@color/white"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/tvReferenceId"
        app:layout_constraintStart_toStartOf="@+id/tvReferenceId"
        app:layout_constraintTop_toBottomOf="@+id/tvReferenceId"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/tvAppCode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sf_pro_text_regular"
        android:includeFontPadding="false"
        android:text="@string/authentication_code"
        android:textColor="@color/hint_color"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/view34_time"
        app:layout_constraintStart_toStartOf="@+id/view34_time"
        app:layout_constraintTop_toTopOf="@+id/view34_time" />

    <TextView
        android:id="@+id/tvAppCodeInfo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:lineSpacingExtra="9sp"
        tools:text="WEDFS45S"
        android:textColor="@color/white"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/tvAppCode"
        app:layout_constraintStart_toStartOf="@+id/tvAppCode"
        app:layout_constraintTop_toBottomOf="@+id/tvAppCode" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout36"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view33_date">

        <TextView
            android:id="@+id/tvMethod"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/sf_pro_text_regular"
            android:includeFontPadding="false"
            android:text="@string/payment_method"
            android:textColor="@color/hint_color"
            android:textSize="@dimen/size12"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvMethod"
            tools:itemCount="2"
            tools:listitem="@layout/payment_items_history" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout36">


        <TextView
            android:id="@+id/textView96"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:fontFamily="@font/sf_pro_text_heavy"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView97"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="KD 9.400" />

        <TextView
            android:id="@+id/textView97"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/sf_pro_text_regular"
            android:text="@string/grand_total"
            android:textColor="@color/hint_color"
            android:textSize="@dimen/size14"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>