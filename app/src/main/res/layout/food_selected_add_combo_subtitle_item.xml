<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/margin10"
    android:backgroundTint="@color/transparent">

    <ImageView
        android:id="@+id/img_sub_item"
        android:layout_width="65dp"
        android:layout_height="56dp"
        android:layout_marginTop="2dp"
        android:adjustViewBounds="true"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/text_sub_add_food_name"
        app:layout_constraintEnd_toEndOf="@+id/view34"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view34"
        app:srcCompat="@drawable/placeholder_icon"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <View
        android:id="@+id/view34"
        android:layout_width="61dp"
        android:layout_height="82dp"
        android:layout_marginBottom="1dp"
        android:layout_marginEnd="45dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_sub_add_food_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="Salt"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/size11"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/view34"
        app:layout_constraintStart_toStartOf="@+id/view34"
        app:layout_constraintTop_toBottomOf="@+id/img_sub_item"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/view22"
        android:layout_width="60dp"
        android:layout_height="3.5dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="@dimen/margin6"
        android:visibility="invisible"
        android:layout_marginTop="@dimen/margin5"
        android:background="@color/text_alert_color_red"
        app:layout_constraintEnd_toEndOf="@+id/text_sub_add_food_name"
        app:layout_constraintStart_toStartOf="@+id/text_sub_add_food_name"
        app:layout_constraintTop_toBottomOf="@+id/text_sub_add_food_name" />

</androidx.constraintlayout.widget.ConstraintLayout>