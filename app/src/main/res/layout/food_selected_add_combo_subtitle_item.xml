<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:backgroundTint="@color/transparent"
    android:layout_margin="@dimen/margin2">

    <ImageView
        android:id="@+id/img_sub_item"
        android:layout_width="80dp"
        android:layout_height="@dimen/margin60"
        android:layout_marginTop="2dp"
        android:adjustViewBounds="true"
        android:maxWidth="110dp"
        android:maxHeight="70dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/text_sub_add_food_name"
        app:layout_constraintEnd_toEndOf="@+id/view34"
        app:layout_constraintStart_toStartOf="@+id/view34"
        app:layout_constraintTop_toTopOf="@+id/view34"
        app:srcCompat="@drawable/app_icon"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <View
        android:id="@+id/view34"
        android:layout_width="100dp"
        android:layout_height="77dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_sub_add_food_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:fontFamily="@font/sf_pro_text_regular"
        android:gravity="center"
        android:letterSpacing="-0.02"
        android:text="Salt"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="@dimen/size12"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/view34"
        app:layout_constraintStart_toStartOf="@+id/view34"
        app:layout_constraintTop_toBottomOf="@+id/img_sub_item"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/view22"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:visibility="invisible"
        android:layout_marginTop="@dimen/margin6"
        android:background="@drawable/red_rectangle"
        app:layout_constraintEnd_toEndOf="@+id/text_sub_add_food_name"
        app:layout_constraintStart_toStartOf="@+id/text_sub_add_food_name"
        app:layout_constraintTop_toBottomOf="@+id/text_sub_add_food_name" />

</androidx.constraintlayout.widget.ConstraintLayout>